      "source": [
        "# swap the values of variables without using temporary variable"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "x=10\n",
        "y=5\n",
        "\n",
        "def swap_variables(x,y):\n",
        "  print(f\"Before swap x: {x} and y: {y}\")\n",
        "\n",
        "  x,y=y,x\n",
        "\n",
        "  print(f\"After swap  x: {x} and y: {y}\")\n",
        "\n",
        "swap_variables(x,y)"
      ],
      "metadata": {
